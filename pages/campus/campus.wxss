.background {
  width: 100%;
  position: relative;
  z-index: 999;
  background: #689bda;
  display: flex;
  flex-flow: row;
}
.background .title{
  font-style: normal;
  color: #fff;
  padding-top: 85rpx;
  font-size: 45rpx;
  font-weight: bold;
}
.background .picture{
  height: 60rpx;
  width: 60rpx;
  padding-top: 85rpx;
  padding-left: 10rpx;
}

.title-text{
  position: relative;
  display: flex;
  margin-left: 70rpx;
}
.title-text .text {
  font-weight: bold;
  width: 100%;
  top: 1em;
  color:#666;
  line-height: 45px;
  font-size: 40rpx;
}


.swiper {
  margin-top: 30rpx;
  height: 400rpx;
  width: 100%;
}
.slide-image{
  height: 400rpx;
  width: 80%;
  border-radius: 20rpx;
}
.center {
  position: relative;
  overflow: hidden;
  align-items: center;
  display: flex;
  justify-content: center;
}


.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 34rpx;
  transform: translateY(-50%);
  transition: 0.3s;
}

.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}

.navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  justify-content: left;
  align-items: center;
}

.navigator:before {
  position: absolute;
  left: 30rpx;
  top: 0;
  height: 1px;
  border-top: 1rpx solid #d8d8d8;
  color: #d8d8d8;
}

.navigator:first-child:before {
  display: none;
}

.title-text .navigator-text {
  position: relative;
  width: 70%;
}
.navigator-time{
  position: relative;
  right: 2px;
  color: #666;
}
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;
}

.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 20rpx;
}

.kind-list-item {
  margin: 20rpx 0;
  background-color: #fff;
  border-radius: 4rpx;
  overflow: hidden;
  border-bottom: 1px dashed #888;
}

.kind-list-item:first-child {
  margin-top: 0;
}

.kind-list-text {
  flex: 1;
  font-weight: bold;
  width: 100%;
  color:#666;
  line-height: 45px;
  font-size: 40rpx;
}

.kind-list-img {
  width: 60rpx;
  height: 60rpx;
}

.kind-list-item-hd {
  display: flex;
  align-items: center;
  transition: opacity 0.3s;
}

.kind-list-item-hd-show {
  opacity: 0.2;
}

.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}

.kind-list-item-bd-show {
  height: auto;
}