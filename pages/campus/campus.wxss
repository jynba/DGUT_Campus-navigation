.kind-list-item {
  height: auto;
  margin: 10rpx 30rpx;
  padding: 25rpx 0;
  background-color: #fff;
  overflow: hidden;
  border-radius: 50rpx;
  box-sizing: border-box;
  position: relative;
}

.kind-list-text {
  margin-top: 10rpx;
  flex: 1;
  font-weight: bold;
  width: 100%;
  color:#666666;
  font-size: 40rpx;
}

.kind-list-img {
  width: 60rpx;
  height: 60rpx;
}

.kind-list-item-hd {
  display: flex;
  align-items: center;
  /* transition: opacity 0.3s; */
}

.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}

.kind-list-item-bd-show {
  height: auto;
}